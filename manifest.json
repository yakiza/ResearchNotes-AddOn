{

	"manifest_version": 2,
	"name": "ResearchNotes",
	"version": "1.0",
	
  
	"description": "A tool that enables you to store  research notes and references",
  
	"icons": {
	  "96": "icons/logo.png"
	},
  
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["index.js"]
	  }
	]
	,
	"browser_action":{
		"default_popup": "/index.html"
	}
	,
	"permissions": [
		"menus",
		"contextMenus",
		"notifications",
        "activeTab"]

  }